{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "r2-gallery-api",
	"main": "src/index.js",
	"compatibility_date": "2025-12-23",
	"observability": {
		"enabled": true
	},
	/**
     * R2 Bucket Binding
     * This connects your Worker to your storage.
     */
	"r2_buckets": [
		{
			"binding": "MY_BUCKET",
			"bucket_name": "discord-media"
		}
	]
}